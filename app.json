{
  "expo": {
    "name": "DietBite Pro",
    "slug": "dietbite-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "enabled": false
    },
    "android": {
      "package": "com.grantedsolutions.dietbitepro",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "creator": "DietBite Pro was created by Kenneth Grant of Granted Solutions."
    },
    "plugins": ["expo-router"]
  }
}
